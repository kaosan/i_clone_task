<h3>以下の内容で、送信する。</h3>

<%= form_with(model: @image, url: images_path, local: true) do |form| %>
<%= image_tag(@image.image.url) %>
<%= form.hidden_field :image_cache %>
  <%= form.hidden_field :content %>

  <p>コメント:<%= @image.content %></p>
  <%= form.submit  "登録する" %>
<% end %>

<%= form_with(model:@image, url:new_image_path, local:true, method:"get") do |form| %>
  <%= form.hidden_field :image_cache %>
  <%= form.submit "戻る", name: "back" %>
<% end %>
