<h1>ログイン</h1>
<%= form_with(scope: :session, url: sessions_path, local: true) do |f| %>
<div class ="login">
<%= f.label :メールアドレス %>
<%= f.email_field :email %>
</div>
<div class="login">
<%= f.label :パスワード %>
<%= f.password_field :password %>
</div>

<%= f.submit "ログイン", class: 'botton' %>
<% end %>
